      	"EXEPATH": "${config:TOOLCHAIN}/bin",
      	"EXE": "",
      	"YOSYS": "${config:EXEPATH}/yosys",
	"NEXTPNR": "${config:EXEPATH}/nextpnr-himbaechel",
	"OFL": "${config:EXEPATH}/openFPGALoader",
	"GMPACK":  "${config:EXEPATH}/gmpack",
	"GTKW": "${config:EXEPATH}/gtkwave",
	"IVL": "${config:EXEPATH}/iverilog",
	"VVP": "${config:EXEPATH}/vvp",
	"IVLFLAGS": "-g2012 -gspecify -Ttyp",
	"GHDL": "${config:EXEPATH}/ghdl",
	"CELLS_SYNTH": "${config:TOOLCHAIN}/share/yosys/gatemate/cells_sim.v",
	"CELLS_IMPL": "${config:TOOLCHAIN}/share/yosys/gatemate/cells_bb.v",
      	"build.spc": " "
      
}
